{
  "$schema": "http://json.schemastore.org/proxies",
  "proxies": {
    "openhack-prod-proxy": {
      "matchCondition": {
        "route": "/proxy/{functionName}"
      },
      "backendUri": "https://localhost/api/{functionName}"
    },
    "openhack-prod-proxy-external": {
      "matchCondition": {
        "route": "/proxy/{resourceName}"
      },
      "backendUri": "https://serverlessohproduct.trafficmanager.net/api/{resourceName}"
    },
    "MockHello": {
      "matchCondition": {
        "route": "/api/hello"
      },
      "responseOverrides": {
        "response.statusCode": "200",
        "response.statusReason": "OK",
        "response.body": "{\n    \"message\": \"Hello\"\n}"
      }
    }
  }
}